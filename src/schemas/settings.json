{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "filterPaneEnabled": {
      "type": "boolean",
      "messages": {
        "type": "filterPaneEnabled must be a boolean"
      }
    },
    "navContentPaneEnabled": {
      "type": "boolean",
      "messages": {
        "type": "navContentPaneEnabled must be a boolean"
      }
    },
    "useCustomSaveAsDialog": {
      "type": "boolean",
      "messages": {
        "type": "useCustomSaveAsDialog must be a boolean"
      }
    },
    "extensions": {
      "type": "array",
      "items": {
        "$ref": "#extension"
      },
      "invalidMessage": "extensions array is invalid"
    },
    "layoutType": {
      "type": "number",
      "enum": [0, 1],
      "default": 0,
      "invalidMessage": "layoutType property is invalid"
    },
    "customLayout": {
      "$ref": "#customLayout"
    }
  }
}